<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="915dp"
    tools:context=".MenuActivity">

    <View
        android:id="@+id/view"
        android:layout_width="410dp"
        android:layout_height="99dp"
        android:background="@drawable/header_red"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView39"
        android:layout_width="59dp"
        android:layout_height="34dp"
        android:layout_marginStart="335dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="16dp"
        android:layout_marginBottom="49dp"
        android:text="Menu"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/view"
        app:layout_constraintEnd_toEndOf="@+id/view"
        app:layout_constraintStart_toStartOf="@+id/view"
        app:layout_constraintTop_toTopOf="@+id/view" />

    <ImageView
        android:id="@+id/ic_back2"
        android:layout_width="28dp"
        android:layout_height="28dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="291dp"
        android:layout_marginBottom="55dp"
        android:src="@android:drawable/ic_menu_revert"
        android:tint="@color/white"
        app:layout_constraintBottom_toBottomOf="@+id/view"
        app:layout_constraintEnd_toStartOf="@+id/textView39"
        app:layout_constraintStart_toStartOf="@+id/view"
        app:layout_constraintTop_toTopOf="@+id/view" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="181dp"
        android:layout_height="33dp"
        android:layout_marginStart="13dp"
        android:layout_marginTop="19dp"
        android:layout_marginEnd="217dp"
        android:text="Personal Informations"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view" />

    <Button
        android:id="@+id/profileBtn"
        android:layout_width="410dp"
        android:layout_height="50dp"
        android:layout_marginTop="64dp"
        android:background="@drawable/bg_button_outline"
        android:text="Personalize Profile"
        android:textAlignment="viewStart"
        android:textColor="#F44336"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view" />

    <Button
        android:id="@+id/Accbtn"
        android:layout_width="410dp"
        android:layout_height="50dp"
        android:layout_marginStart="1dp"
        android:layout_marginTop="7dp"
        android:background="@drawable/bg_button_outline"
        android:text="Account Services"
        android:textAlignment="viewStart"
        android:textColor="#F44336"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/profileBtn" />

    <Button
        android:id="@+id/Cardbtn"
        android:layout_width="410dp"
        android:layout_height="50dp"
        android:layout_marginStart="2dp"
        android:layout_marginTop="9dp"
        android:background="@drawable/bg_button_outline"
        android:text="Card Services"
        android:textAlignment="viewStart"
        android:textColor="#F44336"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Accbtn" />

    <Button
        android:id="@+id/Loanbtn"
        android:layout_width="411dp"
        android:layout_height="51dp"
        android:layout_marginTop="9dp"
        android:background="@drawable/bg_button_outline"
        android:text="Loan Services"
        android:textAlignment="viewStart"
        android:textColor="#F44336"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Cardbtn" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="84dp"
        android:layout_height="30dp"
        android:layout_marginStart="13dp"
        android:layout_marginTop="25dp"
        android:layout_marginEnd="315dp"
        android:text="Security"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Loanbtn" />

    <Button
        android:id="@+id/AccLbtn"
        android:layout_width="410dp"
        android:layout_height="50dp"
        android:layout_marginStart="2dp"
        android:layout_marginTop="10dp"
        android:background="@drawable/bg_button_outline"
        android:text="Account Lock"
        android:textAlignment="viewStart"
        android:textColor="#F44336"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView4" />

    <Button
        android:id="@+id/CardLbtn"
        android:layout_width="410dp"
        android:layout_height="50dp"
        android:layout_marginStart="2dp"
        android:layout_marginTop="7dp"
        android:background="@drawable/bg_button_outline"
        android:text="Card Lock"
        android:textAlignment="viewStart"
        android:textColor="#F44336"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/AccLbtn" />

    <Button
        android:id="@+id/btn"
        android:layout_width="410dp"
        android:layout_height="50dp"
        android:layout_marginTop="12dp"
        android:layout_marginEnd="1dp"
        android:background="@drawable/bg_button_outline"
        android:text="Terms and Conditions"
        android:textAlignment="viewStart"
        android:textColor="#F44336"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView5" />

    <Button
        android:id="@+id/logoutbtn"
        android:layout_width="410dp"
        android:layout_height="50dp"
        android:layout_marginTop="7dp"
        android:background="@drawable/bg_button_outline"
        android:text="Log Out"
        android:textAlignment="viewStart"
        android:textColor="#F44336"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn"
        app:layout_constraintVertical_bias="0.005" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="131dp"
        android:layout_height="36dp"
        android:layout_marginStart="13dp"
        android:layout_marginTop="21dp"
        android:layout_marginEnd="267dp"
        android:text="Documentations"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/CardLbtn" />

</androidx.constraintlayout.widget.ConstraintLayout>